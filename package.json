{
  "name": "respectlytics-react-native",
  "version": "2.0.0",
  "description": "Official Respectlytics SDK for React Native. Privacy-first analytics with automatic session management, offline event queuing, and zero device identifier collection.",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "build": "bob build",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "clean": "rm -rf lib"
  },
  "keywords": [
    "react-native",
    "analytics",
    "privacy",
    "tracking",
    "events",
    "respectlytics",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/respectlytics/respectlytics-react-native.git"
  },
  "author": "Respectlytics <respectlytics@loheden.com>",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/respectlytics/respectlytics-react-native/issues"
  },
  "homepage": "https://github.com/respectlytics/respectlytics-react-native#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-community/netinfo": "^11.3.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.72.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "react": "^18.2.0",
    "react-native": "^0.73.0",
    "react-native-builder-bob": "^0.23.2",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": ">=1.17.0",
    "@react-native-community/netinfo": ">=9.0.0",
    "react": ">=18.0.0",
    "react-native": ">=0.70.0"
  },
  "peerDependenciesMeta": {
    "@react-native-async-storage/async-storage": {
      "optional": false
    },
    "@react-native-community/netinfo": {
      "optional": false
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}

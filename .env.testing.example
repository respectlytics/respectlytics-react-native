# Respectlytics React Native SDK - Test Configuration
# Copy this file to .env.testing and fill in your values
# NEVER commit .env.testing to git

# Required: API key from Respectlytics dashboard
RESPECTLYTICS_TEST_API_KEY=your-test-app-api-key-here

# Test server URL (default: localhost)
RESPECTLYTICS_TEST_BASE_URL=http://localhost:8000/api/v1

# Production server URL (for production testing)
RESPECTLYTICS_PROD_BASE_URL=https://respectlytics.com/api/v1
